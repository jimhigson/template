<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <title>Chart Prototype</title>

   {{#each stylesheets}}
       <link rel="stylesheet" href="{{this}}">
   {{/each}}

   {{#each scripts}}
       <script type="text/javascript" src="{{this}}"></script>
   {{/each}}

</head>

<body>
   <svg id="mainChart" width="1" height="1">
       <defs>
           <linearGradient id="backgroundGradient" x1="0%" y1="0%" x2="0%" y2="100%">
               <stop class="top" offset="0%" />
               <stop class="bottom" offset="100%" />
           </linearGradient>
       </defs>

        <rect class="bg"
             width="800" height="600" x="0" y="0"
       ></rect>

       <g class="data">
       </g>

       <g class="data-overlay">
           <rect></rect>
           <rect></rect>
       </g>

       <g class="axes">
           <g class="x"></g>
           <g class="y"></g>
       </g>


   </svg>
</body>
</html>
